katy_meta_unload.cpp:  katyview.moc.cpp textdocument.moc.cpp katy.moc.cpp katypref.moc.cpp texteditor.moc.cpp
	@echo 'creating katy_meta_unload.cpp'
	-rm -f katy_meta_unload.cpp
	@echo 'static const char * _metalist_katy[] = {' > katy_meta_unload.cpp
	cat  katyview.moc.cpp textdocument.moc.cpp katy.moc.cpp katypref.moc.cpp texteditor.moc.cpp | grep 'char.*className' | sed -e 's/.*[^A-Za-z0-9_:]\([A-Za-z0-9_:]*\)::className.*$$/\"\1\",/' | sort | uniq >> katy_meta_unload.cpp
	@echo '0};' >> katy_meta_unload.cpp
	@echo '#include <kunload.h>' >> katy_meta_unload.cpp
	@echo '_UNLOAD(katy)' >> katy_meta_unload.cpp


